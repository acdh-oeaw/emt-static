<?xml version="1.0" encoding="UTF-8"?>
<TEI xmlns="http://www.tei-c.org/ns/1.0" xml:base="{{ domain }}indices" xml:id="cmif.xml">
    <teiHeader>
        <fileDesc>
            <titleStmt>
                <title type="label">Correspondence Metadata Interchange Format für: Familiensache. Dynastische Handlungsspielräume in der Korrespondenz von Kaiserin Eleonora Magdalena von Pfalz-Neuburg (1655-1720)</title>
                <editor>Katrin Keller<email>katrin.keller@oeaw.ac.at</email></editor>
            </titleStmt>
            <publicationStmt>
                <publisher>Institut für die Erforschung der Habsburgermonarchie und des Balkanraumes</publisher>
                <idno type="url">{{ domain }}cmif.xml</idno>
                <date when-iso="{{ date }}">{{ date }}</date>
                <availability>
                    <licence target="https://creativecommons.org/licenses/by/4.0/">CC-BY</licence>
                </availability>
            </publicationStmt>
            <sourceDesc>
                <bibl xml:id="ad5c20cc-31b0-11ed-baad-f50cc1e65b54" type="online">Familiensache? Die Korrespondenz der Kaiserin Eleonora Magdalena (1655–1720). Bearbeitet von Katrin Keller, Ines Peper, Anna Spitzbart. Wien 2024.</bibl>
            </sourceDesc>
        </fileDesc>
        <profileDesc>
{% for x in data %}{% if x.doc_id %}
            <correspDesc ref="{{ domain }}{{ x.doc_id}}" source="#ad5c20cc-31b0-11ed-baad-f50cc1e65b54">
                <correspAction type="sent">
                    <persName ref="{{ x.sender_gnd }}">{{ x.sender_label }}</persName>{% if x.place_label %}
                    <placeName {% if x.place_geonames %} ref="{{ x.place_geonames }}"{% endif %}>{{ x.place_label }}</placeName>{% endif %}
                    {% if x.date %}{{ x.date|safe }}{% endif %}
                </correspAction>
                <correspAction type="received">
                    <persName ref="{{ x.receiver_gnd }}">{{ x.receiver_label }}</persName>
                </correspAction>
            </correspDesc>{% endif %}{% endfor %}
        </profileDesc>
        </teiHeader>
    <text>
        <body>
            <p>some text</p>
        </body>
    </text>
</TEI>